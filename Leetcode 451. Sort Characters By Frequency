class Solution {
public:
    string frequencySort(string s) {
        
        unordered_map<char , int>mp;

        for(char i : s){
            mp[i]++;
        }

        sort(s.begin() , s.end() , [&](char &a , char &b){
               return (mp[a] == mp[b]) ? a < b : mp[a] > mp[b];
        });
        return s;
    }
};
